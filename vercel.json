{
  "version": 2,
  "functions": {
    "api/index.js": {
      "maxDuration": 60
    },
    "api/health.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/ping",
      "dest": "/api/health.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
